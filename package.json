{
  "name": "blog.yopark.dev",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "velite && next build",
    "content": "velite --clean",
    "start": "next start",
    "format": "prettier --write .",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "update-deps": "ncu -u && bun i",
    "prune-code": "ts-prune -i '.next|.velite|src/app'",
    "prepare": "is-ci || husky"
  },
  "dependencies": {
    "@giscus/react": "latest",
    "@lyket/react": "latest",
    "@svgr/webpack": "latest",
    "clsx": "latest",
    "date-fns": "latest",
    "framer-motion": "latest",
    "next": "latest",
    "react": "latest",
    "react-dom": "latest"
  },
  "devDependencies": {
    "@tailwindcss/typography": "latest",
    "@trivago/prettier-plugin-sort-imports": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "autoprefixer": "latest",
    "class-variance-authority": "latest",
    "eslint": "^8",
    "eslint-config-next": "latest",
    "eslint-plugin-no-relative-import-paths": "latest",
    "husky": "latest",
    "is-ci": "^3.0.1",
    "lint-staged": "latest",
    "postcss": "latest",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "latest",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "latest",
    "rehype-slug": "latest",
    "remark-gfm": "latest",
    "tailwind-merge": "latest",
    "tailwindcss": "latest",
    "ts-prune": "^0.10.3",
    "typescript": "latest",
    "velite": "latest"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
